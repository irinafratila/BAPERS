<AnchorPane prefHeight="825.0" prefWidth="1001.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Admin.deleteUser">
<children>
    <AnchorPane prefHeight="111.0" prefWidth="1001.0" style="-fx-background-color: #7F38EC#7F38EC;">
        <children>
            <ImageView fx:id="brandingImageView" fitHeight="114.0" fitWidth="149.0" layoutX="33.0" layoutY="4.0" pickOnBounds="true" preserveRatio="true">
                <image>
                    <Image url="@../img/logo.png" />
                </image>
            </ImageView>
            <Label layoutX="197.0" layoutY="47.0" text="User Registration" textFill="WHITE">
                <font>
                    <Font name="System Bold" size="24.0" />
                </font>
            </Label>
        </children>
    </AnchorPane>
    <Label layoutX="229.0" layoutY="212.0" prefHeight="23.0" prefWidth="122.0" text="Staff ID" translateY="100.0">
        <font>
            <Font size="18.0" />
        </font>
    </Label>
    <TextField fx:id="id" layoutX="229.0" layoutY="249.0" prefHeight="47.0" prefWidth="581.0" promptText="ID" translateY="100.0" />
    <Button fx:id="createAccountButton" layoutX="354.0" layoutY="461.0" mnemonicParsing="false" onAction="#deleteUserButtonAction" prefHeight="41.0" prefWidth="322.0" text="Delete Account" />
    <Button fx:id="closeButton" layoutX="351.0" layoutY="533.0" mnemonicParsing="false" onAction="#cancelRegister" prefHeight="41.0" prefWidth="322.0" text="Close" />
    <Label fx:id="deleteUserMessageLabel" layoutX="376.0" layoutY="133.0" prefHeight="31.0" prefWidth="301.0" />
</children>
</AnchorPane>
